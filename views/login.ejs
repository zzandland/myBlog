<% include ./partials/header %>
  <div id="login" class="field-form">
 	<h2 class='h3 my-3'>Login</h2>
    <form action="/login" method="POST">
	  <div class="form-group">
	    <input type="text" name="username" class="form-control form-custom" id="username" placeholder="Username">
	  </div>  
	  <div class="form-group">
	    <input type="password" name="password" class="form-control form-custom" id="password" placeholder="Password">
	  </div>  
	  <button type="submit" class="btn-secondary btn">Login!</button>
	</form>	
  </div>
<% include ./partials/footer%>